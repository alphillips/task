{
  "name": "@react-ag-components/header",
  "version": "0.6.3",
  "description": "header React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "npm run build-css && nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-demo": "nwb serve-react-demo",
    "start": "npm-run-all --parallel start-demo watch-css",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "react-burger-menu": "^2.1.4",
    "react-autosuggest": "^9.3.2"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "node-sass": "^4.5.3",
    "nwb": "0.15.x",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^3.0.0"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alphillips/header.git"
  },
  "keywords": [
    "react-component"
  ]
}
